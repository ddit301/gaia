<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="best.gaia.lunch.dao.LunchDao">
	<select id="selectUser" parameterType="string" resultType="best.gaia.vo.MemberVO">
		SELECT *
		FROM MEMBER
		WHERE MEMBER_ID = #{member_id}
	</select>
	
	<select id="selectUserList" resultType="best.gaia.vo.MemberVO">
		SELECT *
		FROM MEMBER
	</select>
	
</mapper>